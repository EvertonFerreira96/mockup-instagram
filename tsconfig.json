{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true, 
    "baseUrl": "./",
    "paths": {
      "@/*":["./src/*"],
      "@main/*": ["./src/main/*"],
      "@domain/*": ["./src/domain/*"],
      "@presentation/*": ["./src/presentation/*"]
    }
  },
  "include": [
     "src/**/*",
     "src/**/*.json"
  ],
  "exclude": ["node_modules/expo/node_modules","node_modules/expo/babel.config.js","node_modules/expo/metro.config.js","node_modules/expo/jest.config.js"], 
  
}